#include <stdio.h>
/*
	Ceci est un commentaire
*/
#define kid "Grace_kid"
#define S "#include <stdio.h>%c/*%c%cCeci est un commentaire%c*/%c#define kid "Grace_kid"%c#define S %c%s;%c%c#define start main%c%cint start() {%c%cFile *f = fopen(kid, "w")%c%cfprintf(f, S, 10, 10, 9, 10, 10, 10, 34, S, 34, 10, 10, 10, 10, 9, 10, 9, 10, 10);%c}%c;"
#define start main

int start() {
	File *f = fopen(kid, "w")
	fprintf(f, S, 10, 10, 9, 10, 10, 10, 34, S, 34, 10, 10, 10, 10, 9, 10, 9, 10, 10);
}
